Algoritmo Saludo
	
	Escribir "Ingresa el nombre:";
	leer nombre;
	
	
	Escribir "Ingresa el apellido:";
	leer apellido;
	
	
	Escribir "Ingresa el direccion:";
	leer direccion;
	
	
	
	Escribir "Nombre:",nombre;

	
	
	Escribir "Apellido:",apellido;

	
	
	Escribir "Direccion:",direccion;

	
FinAlgoritmo
